//// Created by fazalqadir on 1/19/26.//#include <iostream>using namespace std;int main(){    int t;    cin >> t;    for (int i = 0; i < t; i++)    {        string yes;        cin >> yes;        for (int j =0;j<yes.length();j++)        {            if (yes[j]>='A'&&yes[j]<='Z')            {                yes[j]=yes[j]+32;            }         }        if (yes=="yes")        {            cout<<"YES"<<endl;        }        else        {            cout<<"NO"<<endl;        }    }}