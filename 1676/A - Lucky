//// Created by fazalqadir on 1/22/26.//#include <iostream>using namespace std;int main(){Â    int t;    cin >> t;    for (int j =0;j<t;j++){        string str;        cin >> str;        int sum =0,sum1=0;        for (int i =0;i<=5;i++)        {            if (i<3)            {                int t = str[i];                t-=48;                sum+=t;            }            else            {                int t = str[i];                t-=48;                sum1+=t;            }        }        if (sum==sum1)        {            cout << "YES"<<endl;        }        else        {           // cout << sum<<endl<<sum1;            cout << "NO"<<endl;        }    }}